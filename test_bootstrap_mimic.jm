// Mimic compiler structure to verify bootstrapping capability

버퍼 = 준비(100);
개수 = 0;

ㅎㅅ 추가(값) {
    버퍼[개수] = 값;
    개수 = 개수 + 1;
}

ㅎㅅ 처리(문자열) {
    i = 0;
    len = 길이(문자열);

    ㅂㅂ1 (i < len) {
        c = 문자읽기(문자열, i);
        추가(c);
        i = i + 1;
    }
}

// Main logic
입력 = "Test String";
처리(입력);

출력 = "";
j = 0;
ㅂㅂ1 (j < 개수) {
    출력 = 문자연결(출력, 문자(버퍼[j]));
    j = j + 1;
}

ㅍㅇㅊㄹ("mimic_output.txt", 출력);
ㅊㄹ("Mimic Complete");
